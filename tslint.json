{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "ordered-imports": false,
    "no-empty": false,
    "no-submodule-imports": false,
    "member-access": false
  }
}